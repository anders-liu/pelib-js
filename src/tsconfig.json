{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "module": "none",
        "noImplicitReturns": true,
        "outDir": "../out/tsc",
        "removeComments": true,
        "sourceMap": true,
        "strict": true,
        "stripInternal": true,
        "target": "es6"
    },
    "include": [
        "**/*.ts"
    ]
}